<a href="/" class="btn" style="margin-top:5px; margin-left: 1100px;">Logout</a>
<h4>My Result</h4>
<div class="stuTable">
  <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Roll no.</th>
          <th scope="col">Name</th>
          <th scope="col">Date of birth</th>
          <th scope="col">Score</th>
        </tr>
      </thead>
      <tbody>        
        <tr>
          <td><%= one.roll %> </td>
          <td><%= one.name %> </td>
          <td><%=  one.dob.toString().substring(0,10) %> </td>
          <td><%= one.score %> </td>
        </tr>
        
      </tbody>
    </table>    
</div>